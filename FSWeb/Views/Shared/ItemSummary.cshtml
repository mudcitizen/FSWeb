@model ItemSummaryVM
<head>
    <style>
        .floatRight {

        }
    </style>
</head>

<div class="card card-outline-primary m-1 p-1">
    <div class="bg-faded p-1">
        <h4>
            @Model.ItemName
            <span style="float:right;">
                @Model.CategoryName
            </span>
        </h4>
    </div>
    <div class="card-text p-1 ">
        <small>Finds - @Model.FindCount </small>
        <span style="float:right;">
            <small>Last - @Model.LastFind </small>
        </span>
    </div>
</div>